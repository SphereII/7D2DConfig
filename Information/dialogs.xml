      <statement id="statement_-824755831" text="statement_-824755831">
        <response_entry id="response_170492573" ref_text="If she died at the bottom of that well, how did she make the video?" />
      </statement>
      <statement id="statement_808268362" text="statement_808268362">
        <response_entry id="response_692561290" ref_text="Why did she make the video?" />
      </statement>
      <statement id="statement_759720541" text="statement_759720541">
        <response_entry id="response_-2145927645" ref_text="Couldn't or wouldn't?" />
      </statement>
      <statement id="statement_-1960440832" text="statement_-1960440832">
        <response_entry id="response_1507659575" ref_text="Know about her? She's dead. Why would she care?" />
      </statement>
      <statement id="statement_1929307888" text="statement_1929307888">
        <response_entry id="response_1958032391" ref_text="It's not dead then? With everything I've done, it lives?" />
      </statement>
      <statement id="statement_-1778677533" text="statement_-1778677533">
        <response_entry id="response_-1384724228" ref_text="She is still coming for me?" />
      </statement>
      <statement id="statement_-2054310279" text="statement_-2054310279">
        <response_entry id="response_-92236955" ref_text="I've wasted all this time listening to you when I could have been running." />
      </statement>
      <statement id="statement_-1114866312" text="statement_-1114866312">
        <response_entry id="response_-204844403" ref_text="To what end? I'm still dead when she comes for me." />
      </statement>
      <statement id="statement_-586293420" text="statement_-586293420">
        <response_entry id="response_-2064346467" ref_text="You want me to write a book?" />
        <response_entry id="response_-2062358013" ref_text="You know. Isn't that enough?" />
        <response_entry id="response_-780212454" ref_text="I'm done with you. You've killed me." />
      </statement>
      <statement id="statement_1491270808" text="statement_1491270808">
        <response_entry id="response_1441135985" ref_text="It was just an idea..." />
      </statement>
      <statement id="statement_272570392" text="statement_272570392">
        <response_entry id="response_-1829292311" ref_text="What do you mean? How is the video the key?" />
      </statement>
      <statement id="statement_1345208007" text="statement_1345208007">
        <response_entry id="response_949223669" ref_text="How do we do that?" />
      </statement>
      <statement id="statement_1790083874" text="statement_1790083874">
        <response_entry id="response_1043406159" ref_text="I'll find some." />
      </statement>
      <statement id="statement_335517511" text="statement_335517511">
        <response_entry id="response_1082666055" ref_text="[ WAIT ]" />
      </statement>
      <statement id="statement_-1547291539" text="statement_-1547291539">
        <response_entry id="responseID_-1176316112" ref_text="I don't have the DVD." />
        <response_entry id="response_-622069112" ref_text="I have the DVD" />
      </statement>
      <statement id="statement_-443382724" text="statement_-443382724">
        <response_entry id="response_2127147936" ref_text="Thank you for doing this for me." />
      </statement>
      <statement id="statement_258652666" text="statement_258652666">
        <response_entry id="response_1601539022" ref_text="I'm not done yet..." />
      </statement>
      <statement id="statement_843248462" text="statement_843248462">
        <response_entry id="response_1658290028" ref_text="Who did you send it to?" />
      </statement>
      <statement id="statement_1450290941" text="statement_1450290941">
        <response_entry id="response_1753254079" ref_text="Did we just kill someone?" />
      </statement>
      <statement id="statement_305002939" text="statement_305002939">
        <response_entry id="response_1977811390" ref_text="Okay..." />
      </statement>
